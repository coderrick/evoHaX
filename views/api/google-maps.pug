extends ../layout

block content
  style.
    #map {
      width: 100%;
      height: 500px;
    }

  .page-header
    h2
      i.fa.fa-google
      | Transit Map

 
  #map

  script.
    var map;
    var superchargers = [
      {
        location: 'Suburban',
        latitude: 39.9541,
        longitude: -75.1668
      },
      {
        location: '30th Street',
        latitude: 39.9558,
        longitude: -75.1820
      }
    ];

    function initMap() {
      map = new google.maps.Map(document.getElementById('map'), {
        center: { lat: 39.9541, lng: -75.1668 },
        zoom: 15
      });

      superchargers.forEach(function(sc) {
        var marker = new google.maps.Marker({
          position: new google.maps.LatLng(sc.latitude, sc.longitude),
          icon: {
            url: 'data:image/png;base64,',
            scaledSize: new google.maps.Size(25, 25)
          },
          map: map,
          title: sc.location,
          animation: google.maps.Animation.DROP
        });
      });
    }

  script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyCVzUDL6_ZqNEFCBRlmwll8KMqChbKfRMM&callback=initMap')

